   "source": [
    "import numpy as np\n",
    "\n",
    "def gradient(w, X, y):\n",
    "    m = len(y)\n",
    "    y_pred = X.dot(w)\n",
    "    error = y_pred - y\n",
    "    grad = (1/m) * X.T.dot(error)\n",
    "    return grad\n"
   ]
  }
